        <script src="https://code.jquery.com/jquery-migrate-3.1.0.min.js"></script>
        <!-- 1º: informando apenas o token obrigatório de identificação do cliente -->
        <script src="https://www.dataunion.com.br/c2edf325-0e77-434c-8713-29d4b6bbe3e8" type="text/javascript" async></script>
        <script>
            $(function() {
                var $login = $('.navbar .login');

                var sessionUserName = $("#hdnSessionUserName").data('value');

                if (sessionUserName === '' || typeof sessionUserName === 'undefined') {
                    sessionUserName = 'Minha Conta';
                }

                $.ajax({
                    url: 'https://www.cinemark.com.br/cinemark-mania/checklogin',
                    success: function(data) {
                        userLogged = data;
                        $("#userLogin").val(data);

                        if (data) {
                            $login.html(['<a class="btn" href="https://www.cinemark.com.br//cinemark-mania/pagina-inicial">', '<span class="icon icon-user"></span><span class="label">' + sessionUserName + '</span>', '</a>'].join(''));
                        }
                    }
                });

            });
        </script>

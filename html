            function getCookie(c) {
                var a = document.cookie
                  , b = a.indexOf(" " + c + "=");
                -1 == b && (b = a.indexOf(c + "="));
                -1 == b ? a = null : (b = a.indexOf("=", b) + 1,
                c = a.indexOf(";", b),
                -1 == c && (c = a.length),
                a = unescape(a.substring(b, c)));
                return a
            }
            ;

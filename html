                    dataLayer.push({
                        'event': "banner_click",
                        'user_id': userId,
                        'user_id_method': loginMethod,
                        'user_id_anonymous': $('#sessionIdValue').val(),
                        'city_id': "São Paulo",
                        //User navigation city
                        'banner_content': bannerContent,
                        //send the image id or filename
                    });
                }

                    movies.forEach(function(movie, index) {
                        items.push({
                            item_name: movie.Movie.LocalTitle,
                            // Movie name, must be the same in all itemsArray
                            item_id: movie.Movie.MovieId,
                            //Movie Id, must be the same in all itemsArray
                            item_brand: movie.Movie.Distributor,
                            //The distributor
                            item_category: "Movie ticket",
                            //This is loaded when the user selects his ticket. What type of product is being purchased (Movie Ticket or Snackbar)
                            item_category5: movie.Label || "Em cartaz",
                            index,
                            //If associated with a list. Where the movie was placed in the list.
                            quantity: 1,
                            //When it is not in the ticket selection, keep 1
                            moviegenre: movie.Movie.Genres[0].Description || "",
                            //Movie genre
                            city_id: "São Paulo",
                            //User navigation city
                            theater_id: movie.TheaterId,
                            //If the user has selected a theater already or navigating through the theater options
                            item_list_name: 'Home | Em cartaz'
                        })
                    })
                    return items;
                }

                var userId = 0 != 0 ? 0 : '';
                var loginMethod = 0 != 0 ? 'Email' : '';
                window.onload = function(e) {
                    $("#modal-alert").modal();
                }
                window.onpageshow = function(e) {
                    dataLayer.push({
                        'event': "home",
                        'user_id': userId,
                        'user_id_method': loginMethod,
                        'user_id_anonymous': $('#sessionIdValue').val(),
                        'city_id': "São Paulo",
                        //User navigation city
                    })
                    dataLayer.push({
                        'event': "view_item_list",
                        'user_id': userId,
                        'user_id_method': loginMethod,
                        'user_id_anonymous': $('#sessionIdValue').val(),
                        'city_id': "São Paulo",
                        //User navigation city
                        'ecommerce': {
                            item_list_name: 'Home | Em cartaz',
                            items: getCurrentMoviesItems()
                        }
                    });
                    dataLayer.push({
                        'event': "view_item_list",
                        'user_id': userId,
                        'user_id_method': loginMethod,
                        'user_id_anonymous': $('#sessionIdValue').val(),
                        'city_id': "São Paulo",
                        //User navigation city
                        'ecommerce': {
                            item_list_name: 'Home | Em breve',
                            items: getComingSoonItems()
                        }
                    });
                }
                ;
